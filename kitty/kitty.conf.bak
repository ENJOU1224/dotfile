term xterm-256color

# Fonts
# font_family       GeistMono NFM ,Sarasa Mono SC
# bold_font         GeistMono NFM Bold
# italic_font       CommitMono Nerd Font Italic
# bold_italic_font  CommitMono Nerd Font Bold Italic

#symbol_map U+4E00-U+9FA5 Sarasa Mono SC 

#font_family postscript_name=GeistMono



# BEGIN_KITTY_FONTS
font_family      family="CommitMono Nerd Font"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS


# BEGIN_KITTY_THEME
# Default
include current-theme.conf
# END_KITTY_THEME